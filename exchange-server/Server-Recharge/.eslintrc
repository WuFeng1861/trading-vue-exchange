{
  "env": {
    "node": true,
    "browser": true
  },
  "globals": {
    "exampleGlobalVariable": true
  },
  "rules": {
    "semi": "error",
    "no-var": "error",
    "semi-spacing": "error",
    "comma-spacing": "error",
    "complexity": ["error", 20],
    "max-depth": ["error", 4]
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": []
}
